package HF01;

public class cicas {
	
	int lives;
	String name;
	boolean alive;
	public int getLives() {
		return lives;
	}
	public void setLives(int lives) {
		this.lives = lives;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public boolean isAlive() {
		return alive;
	}
	public void setAlive(boolean alive) {
		this.alive = alive;
	}
	public cicas(int lives, String name, boolean alive) {
		super();
		this.lives = lives;
		this.name = name;
		this.alive = alive;
	}
	
	
	
	
	
}
